<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.victor.gestordedemandas" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/iconstf"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme" >
        <activity android:name="com.victor.gestordedemandas.view.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.victor.gestordedemandas.view.ListarDemandasActivity"
            android:label="Listar Demandas"
            android:parentActivityName="com.victor.gestordedemandas.view.MainActivity" />
        <activity
            android:name="com.victor.gestordedemandas.view.CadastroActivity"
            android:label="Cadastrar"
            android:parentActivityName="com.victor.gestordedemandas.view.MainActivity" />
        <activity android:name="com.victor.gestordedemandas.view.CadastrarGerentes"
            android:parentActivityName="com.victor.gestordedemandas.view.MainActivity"
            android:label="Cadastrar Gerente">
        </activity>
    </application>

</manifest>
